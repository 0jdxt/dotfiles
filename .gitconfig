# This is Git's per-user configuration file.
[user]
	name = Jay
	email = jytrn@protonmail.com
[cola]
	spellcheck = false
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[pull]
	rebase = true
[alias]
	s = status
	f = fetch
	st = stash
	p = push
	pl = pull
	d = diff
	a = add
	c = commit
[core]
    pager = delta
[interactive]
    diffFilter = delta --color-only
[delta]
    side-by-side = true
    line-numbers-left-format = ""
    line-numbers-right-format = "| "
    sytax-theme = Nord

